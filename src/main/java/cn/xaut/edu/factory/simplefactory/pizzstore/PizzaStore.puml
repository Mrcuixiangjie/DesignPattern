@startuml
'https://plantuml.com/class-diagram
abstract class Pizza{
    + prepare():void
    + bake():void
    + cut():void
    + box():void
}
class CheesePizza extends Pizza{
    + prepare():void
    + bake():void
    + cut():void
    + box():void
}
class GreekPizza extends Pizza{
    + prepare():void
    + bake():void
    + cut():void
    + box():void
}
@enduml